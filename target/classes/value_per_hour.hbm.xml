<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
 	<class name="com.EntityClasses.Value_Per_Hour" table="value_per_hour">
 	 	
 	 <id name="id" column="id" >    
 	    <generator class="identity"/>    
 	 </id>
    <property name="semester_id"  column="semester_id"/>
    <property name="value" column="value"/>
    <property name="created_at" column="created_at"/>
    <property name="updated_at" column="updated_at"/>
    <property name="batch_id" column="batch_id"/>
 	</class>
</hibernate-mapping>