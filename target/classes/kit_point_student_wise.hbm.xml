<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
 	<class name="com.EntityClasses.KIT_Point_Student_Wise" table="kit_point_student_wise">
 	 	
 	 <id name="id" column="id" >    
 	    <generator class="assigned"/>    
 	 </id>
 	 	
    <property name="user_id"  column="user_id"/>
    <property name="project_id" column="project_id"/>
    <property name="kit_point" column="kit_point"/>
    <property name="hours" column="hours"/>
    <property name="created_at" column="created_at"/>
    <property name="updated_at" column="updated_at"/>
 	</class>
</hibernate-mapping>