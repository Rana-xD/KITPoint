<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
 	<class name="com.EntityClasses.Budget" table="Budget">
 	<id name="id" column="id" >    
 	    <generator class="assigned"/>    
 	 </id>
    <property name="budget_id"  column="budget_id"/>
    <property name="project_id" column="project_id"/>
    <property name="cost" column="cost"/>
    <property name="created_at" column="created_at"/>
    <property name="updated_at" column="updated_at"/>
 	</class>
</hibernate-mapping>