<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
 	<class name="com.EntityClasses.Project_Master" table="project_master">
 	 	
 	 <id name="id" column="id" >    
 	    <generator class="identity"/>    
 	 </id>
    <property name="project_name"  column="project_name"  />
    <property name="project_code" column="project_code"/>
    <property name="skillset" column="skillset"/>
    <property name="project_type" column="project_type"/>
    <property name="project_co" column="project_co"/>
    <property name="project_leader" column="project_leader"/>
    <property name="project_member" column="project_member"/>
    <property name="description" column="description"/>
    <property name="status" column="status"/>
    <property name="initially_planned" column="initially_planned"/>
    <property name="budget" column="budget"/>
    <property name="kit_point" column="kit_point"/>
    <property name="deadline" column="deadline"/>
    <property name="start_date" column="start_date"/>
    <property name="end_date" column="end_date"/>
    <property name="created_at" column="created_at"/>
    <property name="updated_at" column="updated_at"/>
 
    
 	</class>
</hibernate-mapping>